<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Suppress EI_EXPOSE_REP warnings globally -->
    <!-- These warnings are false positives as the X.509-Attacker framework intentionally 
         exposes mutable objects to allow runtime modification of certificate structures -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>
</FindBugsFilter>